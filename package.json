{
  "name": "node",
  "version": "1.4.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "npm run build && node ./dist/main/serve.js",
    "dev": "sucrase-node src/main/serve.ts",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy dist/main/serve.js",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --runInBand --no-cache --detectOpenHandles --forceExit",
    "test:verbose": "jest --passWithNoTests --runInBand --detectOpenHandles ",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:unitv": "jest --passWithNoTests --runInBand --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage --detectOpenHandles",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "clean-jest": "jest --clearCache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongodb": "^4.0.7",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.3.6",
    "jest": "^27.5.1",
    "lint-staged": "^11.2.3",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "coveralls": "^3.1.1",
    "express": "^4.17.1",
    "fast-glob": "^3.2.7",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.7.3",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.14",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": "14.x"
  }
}
